#!/usr/bin/env node

/**
 * @file Run express.js
 * @author Fikri Rahmat Nurhidayat
 */

// Environment Configuration
require("dotenv").config();

const app = require("../app");
const port = process.env.PORT || 8000;

app.listen(port, () => {
  console.log(`Listening on http://localhost:${port}`);
});
